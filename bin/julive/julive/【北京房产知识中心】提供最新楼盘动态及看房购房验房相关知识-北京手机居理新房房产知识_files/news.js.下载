$(function(){function i(i,s){i.delegate("li","click",function(i){var a=$(this).find("a").html();$(this).parents("li").find(s).html(a),event.stopPropagation(),n()})}function n(){a.find(".show").hide(),a.children("div").find("span").removeClass("tab-on"),o.hide(),d.css("overflow","visible")}var s=$(".tab-top"),a=s.find("li"),o=$("#prop"),d=$("body"),l=$(".ipt-area").find("input");a.click(function(){$(this).children("div").find("span").addClass("tab-on"),$(this).siblings("li").children("div").find("span").removeClass("tab-on"),$(this).children(".show").show(),$(this).siblings("li").children(".show").hide(),d.css("overflow","hidden"),o.show()}),o.click(function(){n()}),l.click(function(){n()}),$(".n-media").hover(function(){$(this).addClass("media-on").siblings().removeClass("media-on")});var e=$(".ana-ul"),t=$(".adv-ul"),h=$(".analyze-h"),c=$(".advisory-h");i(e,h),i(t,c)});