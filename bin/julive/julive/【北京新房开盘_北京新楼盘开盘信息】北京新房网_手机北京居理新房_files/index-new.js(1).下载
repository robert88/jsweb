function adjust(){var t=navigator.userAgent.toLowerCase();/android/.test(t)?$("html").attr("data-type",1):/ios/.test(t)&&$("html").attr("data-type",2)}function AutoScroll(t){$(t).find("ul:first").animate({marginTop:"-.95rem"},2e3,function(){$(this).css({marginTop:"0"}).find("li:first").appendTo(this),setTimeout('AutoScroll(".scrollDiv")',2e3)})}function changePoistion(t,o){if($(window).scrollTop()<t.outerHeight(!0)){if(!o.hasClass("fadeDown"))return!1;o.addClass("fadeUp"),o.removeClass("fadeDown")}else o.addClass("fadeDown"),o.removeClass("fadeUp")}var findItem=$(".find-item");findItem.each(function(){var t=$(this),o=t.find(".open"),e=o.find(".ico-down");o.on("click",function(){e.hasClass("rotate")?t.removeAttr("style"):t.css({height:"auto"}),e.toggleClass("rotate")})}),$(function(){1!=$(".scrollDiv").find("li").length&&setTimeout('AutoScroll(".scrollDiv")',2e3),adjust();var t=($(".container"),$(".topbar-juli")),o=$(".topar-download");if(0==t.length)return!1;$(window).scroll(function(){changePoistion(o,t)})});